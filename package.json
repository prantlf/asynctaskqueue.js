  "version"       : "1.0.0",
  "dependencies": {
    "underscore" : "1.4.4 - 1.6.0",
    "jquery"     : "1.8.0 - 1.11.0"
  },
  "devDependencies": {
    "docco"     : "0.6.x",
    "phantomjs" : "1.9.7-x",
